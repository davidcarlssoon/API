  "paths": {
    "/public/messages": {
      "get": {
        "tags": ["message-controller"],
        "operationId": "getAllMessages",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/MessageList"
                }
              }
            }
          }
        }
      }
    },
    "/public/messages/{id2}": {
      "get": {
        "tags": ["message-controller"],
        "operationId": "getOneMessage",
        "parameters": [
          {
            "name": "id2",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Message"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Message": {
        "type": "object",
        "properties": {
          "id2": {
            "type": "integer",
            "format": "int32"
          },
          "shortId": {
            "type": "integer",
            "format": "int32"
          },
          "headline": {
            "type": "string"
          },
          "text": {
            "type": "string"
          },
          "hyperlink": {
            "type": "string"
          },
          "author": {
            "type": "string"
          },
          "startDate": {
            "type": "string"
          },
          "endDate": {
            "type": "string"
          },
          "publishingDate": {
            "type": "string"
          },
          "logo": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        }
      },
      "MessageList": {
        "type": "object",
        "properties": {
          "messages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Message"
            }
          }
        }
      }
    }
  }
}
